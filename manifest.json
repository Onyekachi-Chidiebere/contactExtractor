{
    "name":"contactExtractor",
    "version":"1.0",
    "description":"extracts phone and emails",
    "browser_action":
    {
      "default_icon": "ico.png"
    },
    "background":
  {
    "scripts": ["function.js"]
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "run_at":"document_end",
    "js": ["jQuery.js"]
  }],
    "manifest_version": 2,
    
    "permissions": [
        "tabs",
        "<all_urls>"
    ]
}
   

    
